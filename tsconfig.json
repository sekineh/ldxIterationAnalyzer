{
    "version": "1.8.9",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": false
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./app.ts",
        "./test/app_test.ts",
        "./typings/browser.d.ts",
        "./typings/browser/ambient/jquery/index.d.ts",
        "./typings/browser/ambient/nvd3/index.d.ts",
        "./typings/browser/ambient/power-assert/index.d.ts",
        "./typings/browser/definitions/d3/index.d.ts",
        "./typings/d3/d3.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/main.d.ts",
        "./typings/main/ambient/jquery/index.d.ts",
        "./typings/main/ambient/nvd3/index.d.ts",
        "./typings/main/ambient/power-assert/index.d.ts",
        "./typings/main/definitions/d3/index.d.ts",
        "./typings/mithril/mithril.d.ts",
        "./typings/tsd.d.ts"
    ],
    "compileOnsave": true
}
